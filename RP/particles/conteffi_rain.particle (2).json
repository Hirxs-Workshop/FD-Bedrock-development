{
	"format_version": "1.10.0",
	"particle_effect": {
		"description": {
			"identifier": "fb:conteffi_rain",
			"basic_render_parameters": {
				"material": "particles_blend",
				"texture": "textures/blocks/confetti/3"
			}
		},
		"components": {
			"minecraft:emitter_rate_instant": {
				"num_particles": 100
			},
			"minecraft:emitter_lifetime_once": {
				"active_time": 10
			},
			"minecraft:emitter_shape_sphere": {
				"offset": [0, 2, 0],
				"radius": 1.5,
				"direction": "outwards"
			},
			"minecraft:particle_lifetime_expression": {
				"max_lifetime": 15
			},
			"minecraft:particle_initial_spin": {
				"rotation": "math.random(-180,180)"
			},
			"minecraft:particle_initial_speed": 20,
			"minecraft:particle_motion_dynamic": {
				"linear_acceleration": ["math.sin(v.particle_age * 90)", -2, "math.cos(v.particle_age * 40)"],
				"linear_drag_coefficient": 5
			},
			"minecraft:particle_appearance_billboard": {
				"size": [0.25, 0.25],
				"facing_camera_mode": "rotate_xyz",
				"uv": {
					"texture_width": 16,
					"texture_height": 16,
					"uv": ["math.floor(v.particle_random_3*2)*8", "math.floor(v.particle_random_4*2)*8"],
					"uv_size": [8, 8]
				}
			},
			"minecraft:particle_motion_collision": {
				"collision_drag": 10,
				"collision_radius": 0.2
			},
			"minecraft:particle_appearance_lighting": {},
			"minecraft:particle_appearance_tinting": {
				"color": {
					"interpolant": "v.particle_age / v.particle_lifetime",
					"gradient": {
						"0.0": "#ffffffff",
						"0.85": "#ffffffff",
						"1.0": "#00FFFFFF"
					}
				}
			}
		}
	}
}